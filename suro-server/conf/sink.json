{
    "default": {
        "type": "local",
        "outputDir": "/tmp/suroserver/default",
        "writer": {
            "type": "text"
        }
    },
    "s3-sink": {
        "type": "s3",
        "bucket": "scm-suro-test",
        "prefixFormatter": {
            "type": "dynamic",
            "format": "date('es/sgm'/yyyy/yyyyMM/yyyyMMdd)"
        },
        "localFileSink": {
            "type": "local",
            "outputDir": "/tmp/suroserver/s3",
            "minPercentFreeDisk": 10,
            "writer": {
                "type": "text"
            }
        }
    },
    "sqs-sink": {
        "type": "sqs",
        "region": "eu-west-1",
        "client.id": "sqs-sink-id",
        "queueName": "sqs_sink",
        "outputDir": "/tmp/suroserver/logger",
        "writer": {
            "type": "text"
        }
    }
}
